<hc:Window x:Class="DiWpfApp.ThirdForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiWpfApp"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        mc:Ignorable="d"
        FontSize="24"
        Background="{DynamicResource ThirdlyRegionBrush}"
        FontFamily="Hacen Tunisia"
        WindowStartupLocation="CenterScreen"
        Title="ThirdForm" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" 
                    VerticalAlignment="Center"
                    HorizontalAlignment="Center">
            <TextBlock Foreground="Yellow">Third Form</TextBlock>
        </StackPanel>
    </Grid>
</hc:Window>
